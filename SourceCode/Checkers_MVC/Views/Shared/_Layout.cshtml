<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv='cache-control' content='no-cache'>
    <meta http-equiv='expires' content='0'>
    <meta http-equiv='pragma' content='no-cache'>

    <title>@ViewBag.Title</title>
    @*<link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />*@
    @{Random rand = new Random(); }
    <link href="@Url.Content("~/Content/board/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/board/board.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/board/checker.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.16.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/board/start.js?nocahe_id=" + rand.Next(0, 1000000).ToString())" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/board/board.js?nocahe_id=" + rand.Next(0, 1000000).ToString())" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/board/execute_ajax.js?nocahe_id=" + rand.Next(0, 1000000).ToString())" type="text/javascript"></script>
    @*<script src="@Url.Content("~/Scripts/modernizr-2.0.6-development-only.js")" type="text/javascript"></script>*@
</head>
<body>
    @RenderBody()
</body>
</html>
